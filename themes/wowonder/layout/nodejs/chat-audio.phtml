<div id="audio_m_{{rand}}">
    <audio crossorigin preload="none">
        <source src="{{fileName}}" type="audio/mpeg">
        <source src="{{fileName}}" type="audio/mp3">
    </audio>
</div>
    
<script>
    $(function() {
        new GreenAudioPlayer('#audio_m_{{rand}}', { showTooltips: true, showDownloadButton: false, enableKeystrokes: true });
    });
</script>